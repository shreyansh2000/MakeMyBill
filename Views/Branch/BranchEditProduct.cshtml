@model MakeMyBill.Models.CartMaster

@{
    ViewData["Title"] = "BranchEditProduct";
    Layout = "~/Views/Shared/_BranchLayout.cshtml";
}


		<!-- Custom Theme files -->
	<link href="~/Login/css/style.css" rel="stylesheet" type="text/css" media="all" />
	<link href="~/Login/css/font-awesome.min.css" rel="stylesheet" type="text/css" media="all" />

	<!-- //Custom Theme files -->

	<!-- web font -->
	<link href="//fonts.googleapis.com/css?family=Hind:300,400,500,600,700" rel="stylesheet">
	<!-- //web font -->

@using(Html.BeginForm("BranchEditProduct","Branch",FormMethod.Post))
{

<div class="w3layouts-main"> 
	<div class="bg-layer">
		<h1>Edit Product</h1>
		<div class="header-main">
			<div class="main-icon">
				<span class="fa fa-eercast"></span>
			</div>
			<div class="header-left-bottom">
				 <input type="hidden" id="Cartid" name="Cartid" value=@Model.Cartid >
				  <input type="hidden" id="Customerid" name="Customerid" value=@Model.Customerid>
					<div class="icon1">
						<span class="fa fa-user"></span>
						<input type="text" placeholder="Quantity" id="ScQty" name="ScQty" value=@Model.ScQty>
					</div>
					<div class="bottom">
                            <input type="submit" class="btn btn-primary" value="Edit">
                     </div>
			</div>
			
		</div>
		
	</div>
</div>	
}



