@model MakeMyBill.Models.ProductMainCategoryMaster
@{
    ViewData["Title"] = "CompanyEditCategory";
    Layout = "~/Views/Shared/_CompanyLayout.cshtml";
}





		<!-- Custom Theme files -->
	<link href="~/Login/css/style.css" rel="stylesheet" type="text/css" media="all" />
	<link href="~/Login/css/font-awesome.min.css" rel="stylesheet" type="text/css" media="all" />

	<!-- //Custom Theme files -->

	<!-- web font -->
	<link href="//fonts.googleapis.com/css?family=Hind:300,400,500,600,700" rel="stylesheet">
	<!-- //web font -->

@using(Html.BeginForm("CompanyEditCategory","Company",FormMethod.Post, new {  enctype= "multipart/form-data"}))
{
<div class="w3layouts-main"> 
	<div class="bg-layer">
		<h1>Edit Category</h1>
		<div class="header-main">
			<div class="main-icon">
				<span class="fa fa-eercast"></span>
			</div>
			<div class="header-left-bottom">
				 <input type="hidden" id="Catid" name="Catid" value=@Model.Catid >

					<div class="icon1">
						<span class="fa fa-user"></span>
						<input type="text" placeholder="Category Name" id="Catname" name="Catname" value=@Model.Catname />
					</div>
					<div class="icon1">
						<span class="fa fa-book"></span>
						<input type="file" id="Catimage" name="Catimage" accept="image/*"/>
					</div>
					
					<div class="bottom">
                            <input type="submit" class="btn btn-primary" value="Add">
                     </div>
			</div>
			
		</div>
		
	</div>
</div>	
}




